@ARTICLE{Soyster1973ConvexProgrammingwith,
  author = {Soyster, A. L.},
  title = {Convex Programming with Set-Inclusive Constraints and Applications
	to Inexact Linear Programming},
  journal = {Operations Research},
  year = {1973},
  volume = {21},
  pages = {1154--1157},
  number = {5},
  month = sep,
  abstract = {This note formulates a convex mathematical programming problem in
	which the usual definition of the feasible region is replaced by
	a significantly different strategy. Instead of specifying the feasible
	region by a set of convex inequalities, $f_{i}(x)\leq b_{i}$, $i=1,2,\cdots,m$,
	the feasible region is defined via set containment. Here n convex
	activity sets $\{K_{j},\ j=1,2,\cdots,n\}$ and a convex resource
	set K are specified and the feasible region is given by $X=\{x \in
	R^{n}|x_{1}K_{1}+x_{2}K_{2}+\cdots +x_{n}K_{n}\subseteq K,\ x_{j}\geq
	0\}$, where the binary operation + refers to addition of sets. The
	problem is then to find $\overline{x}\in X$ that maximizes the linear
	function $c\cdot x$. When the resource set has a special form, this
	problem is solved via an auxiliary linear-programming problem and
	application to inexact linear programming is possible.},
  file = {Soyster1973ConvexProgrammingwithSet-InclusiveConstraintsandApplicationstoInexactLinearProgramming.pdf:\\papers\\Soyster1973ConvexProgrammingwithSet-InclusiveConstraintsandApplicationstoInexactLinearProgramming.pdf:PDF;Soyster1973ConvexProgrammingwithSet-InclusiveConstraintsandApplicationstoInexactLinearProgramming.pdf:\\papers\\Soyster1973ConvexProgrammingwithSet-InclusiveConstraintsandApplicationstoInexactLinearProgramming.pdf:PDF},
