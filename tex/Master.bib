  journal = {Annals of Operations Research},
  year = {1995},
  volume = {56},
  pages = {15-38},
  abstract = {One of the main methods for solving stochastic programs is approximation
	by discretizing the probability distribution. However, discretization
	may lose differentiability of expectational functionals. The complexity
