@ARTICLE{Nemirovski.Rothblum1999complexityofmatrix,
  author = {Nemirovski, A. and Rothblum, U.},
  title = {On complexity of matrix scaling},
  journal = {Linear Algebra and Its Applications},
  year = {1999},
  volume = {303},
  pages = {435-460},
  abstract = {The Line Sum Scaling problem for a nonnegative matrix A is to find
	positive definite diagonal matrices Y, Z which result in prescribed
	row and column sums of the scaled matrix YAZ. The matrix Balancing
	problem for a nonegative square matrix A is to find a positive definite
	diagonal Matrix X such that the row sums in the scaled matrix XAX
	are equal to the corresponding column sums. We demonstrate that epsilon-versions
	of both these problems, same as those of other scaling problems for
	nonnegative multiindex arrays, can be reduced to a specific Geometric
	Programming problem. For the latter problem, we develop a polynomial-time
	algorithm, thus deriving polynomial time solvability of a number
	of generic scaling problems for nonnegative multiindex arrays. Our
	results extend those previously known for the problems of matrix
	balancing [3] and of double-stochastic scaling of a square nonnegative
	matrix [2]. (C) 1999 Published by Elsevier Science Inc. All rights
	reserved.},
  keywords = {matrix scaling matrix balancing polynomial-time complexity},
  owner = {mehrotra},
  timestamp = {2008.12.14}
}

