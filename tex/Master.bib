@ARTICLE{Bertsimas.Boer2005Simulation-basedbookinglimits,
  author = {Bertsimas, D. and de Boer, S.},
  title = {Simulation-based booking limits for airline revenue management},
  journal = {Operations Research},
  year = {2005},
  volume = {53},
  pages = {90-106},
  number = {1},
  abstract = {Deterministic mathematical programming models that capture network
	effects play a predominant role in the theory and practice of airline
	revenue management. These models do not address important issues
	like demand uncertainty, nesting, and the dynamic nature of the booking
	process. Alternatively, the network problem can be broken down into
	leg-based problems for which there are satisfactory solution methods,
	but this approach cannot be expected to capture all relevant network
	aspects. In this paper, we propose a new algorithm that addresses
	these issues. Starting with any nested booking-limit policy, we combine
	a stochastic gradient algorithm and approximate dynamic programming
	ideas to improve the initial booking limits. Preliminary simulation
	experiments suggest that the proposed algorithm can lead to practically
	significant revenue enhancements.},
  keywords = {comparing decision rules seat inventory control fare classes yield-management
	overbooking allocation model situations},
  owner = {mehrotra},
  timestamp = {2008.12.14}
}

