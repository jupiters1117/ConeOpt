@ARTICLE{Morton.Popova2004Bayesianstochasticprogramming,
  author = {Morton, D. P. and Popova, E.},
  title = {A Bayesian stochastic programming approach to an employee scheduling
	problem},
  journal = {Iie Transactions},
  year = {2004},
  volume = {36},
  pages = {155-167},
  number = {2},
  abstract = {Bayesian forecasting models provide distributional estimates for random
	parameters, and relative to classical schemes, have the advantage
	that they can rapidly capture changes in nonstationary systems using
	limited historical data. Unlike deterministic optimization, stochastic
	programs explicitly incorporate distributions for random parameters
	in the model formulation, and thus have the advantage that the resulting
	solutions more fully hedge against future contingencies. In this
	paper, we exploit the strengths of Bayesian prediction and stochastic
	programming in a rolling-horizon approach that can be applied to
	solve real-world problems. We illustrate the methodology on an employee
	production scheduling problem with uncertain up-times of manufacturing
	equipment and uncertain production rates. Computational results indicate
	the value of our approach.},
  keywords = {regularized decomposition method linear-programs fixed recourse optimization
	simulation behavior models times sum},
  owner = {mehrotra},
  timestamp = {2008.12.14}
}

