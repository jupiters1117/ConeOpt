@ARTICLE{Nielsen.Zenios1993MassivelyParallelAlgorithm,
  author = {Nielsen, Soren S. and Zenios, Stavros A.},
  title = {A Massively Parallel Algorithm For Nonlinear Stochastic Network Problems},
  journal = {Operations Research},
  year = {1993},
  volume = {41},
  pages = {319--337},
  number = {2},
  month = mar,
  abstract = {We develop an algorithm for solving nonlinear, two-stage stochastic
	problems with network recourse. The algorithm is based on the framework
	of row-action methods. The problem is formulated by replicating the
	first-stage variables and then adding nonanticipativity side constraints.
	A series of (independent) deterministic network problems are solved
	at each step of the algorithm, followed by an iterative step over
	the nonanticipativity constraints. The solution point of the iterates
	over the nonanticipativity constraints is obtained analytically.
	The row-action nature of the algorithm makes it suitable for parallel
	implementations. A data representation of the problem is developed
	that permits the massively parallel solution of all the scenario
	subproblems concurrently. The algorithm is implemented on a Connection
	Machine CM-2 with up to 32K processing elements and achieves computing
	rates of 276 MFLOPS. Very large problems-8,192 scenarios with a deterministic
	equivalent nonlinear program with 868,367 constraints and 2,474,017
	variables-are solved within a few minutes. We report extensive numerical
	results regarding the effects of stochasticity on the efficiency
	of the algorithm.},
  file = {NielsenZenios1993_AMassivelyParallelAlgorithmForNonlinearStochasticNetworkProblems.pdf:papers2\\NielsenZenios1993_AMassivelyParallelAlgorithmForNonlinearStochasticNetworkProblems.pdf:PDF;NielsenZenios1993AMassivelyParallelAlgorithmForNonlinearStochasticNetworkProblems.pdf:\\papers2\\NielsenZenios1993AMassivelyParallelAlgorithmForNonlinearStochasticNetworkProblems.pdf:PDF},
  issn = {0030-364X},
  keywords = {Computers/computer science: massively parallel, Networks/graphs: stochastic
	generalized networks, Programming: stochastic, nonlinear algorithms,
	Computing},
  owner = {Me},
  publisher = {INFORMS},
  timestamp = {2006.10.25},
  url = {http://links.jstor.org/sici?sici=0030-364X%28199303%2F04%2941%3A2%3C319%3AAMPAFN%3E2.0.CO%3B2-4}
}

