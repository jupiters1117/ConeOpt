@ARTICLE{Iyengar2005Robustdynamicprogramming,
  author = {Iyengar, G. N.},
  title = {Robust dynamic programming},
  journal = {Mathematics of Operations Research},
  year = {2005},
  volume = {30},
  pages = {257-280},
  number = {2},
  abstract = {In this paper we propose a robust formulation for discrete time dynamic
	programming (DP). The objective of the robust formulation is to systematically
	mitigate the sensitivity of the DP optimal policy to ambiguity in
	the underlying transition probabilities. The ambiguity is modeled
	by associating a set of conditional measures with each state-action
	pair. Consequently, in the robust formulation each policy has a set
	of measures associated with it. We prove that when this set of measures
	has a certain "rectangularity" property, all of the main results
	for finite and infinite horizon DP extend to natural robust counterparts.
	We discuss techniques from Nilim and El Ghaoui [17] for constructing
	suitable sets of conditional measures that allow one to efficiently
	solve for the optimal robust policy. We also show that robust DP
	is equivalent to stochastic zero-sum games with perfect information.},
  keywords = {dynamic programming robust optimization markov decision processes
	ambiguity},
  owner = {mehrotra},
  timestamp = {2008.12.14}
}

