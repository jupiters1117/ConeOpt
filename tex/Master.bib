@ARTICLE{Birge.Rosa1996Incorporatinginvestmentuncertainty,
  author = {Birge, J. R. and Rosa, C. H.},
  title = {Incorporating investment uncertainty into greenhouse policy models},
  journal = {Energy Journal},
  year = {1996},
  volume = {17},
  pages = {79-90},
  number = {1},
  abstract = {Greenhouse gas policy decisions require comprehensive understanding
	of atmospheric, economic, and social impacts. Many studies have considered
	the effects of atmospheric uncertainty in global warming, but economic
	uncertainties have received less analysis. We consider a key component
	of economic uncertainty: the return on investments in new technologies.
	Using a mathematical programming model, we show that ignoring uncertainty
	in technology investment policy may lead to decreases as great as
	2 percent in overall expected economic activity in the U.S. with
	even higher losses in possible future scenarios. These results indicate
	that both federal and private technology investment policies should
	be based on models explicitly incorporating uncertainty.},
  keywords = {stochastic linear-programs nested decomposition recourse},
  owner = {mehrotra},
